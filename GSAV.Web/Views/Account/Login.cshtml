    <link rel="SHORTCUT ICON" type="image/x-icon" href="~/Content/upc/ui/bb-icon2.ico">
    <link rel="stylesheet" type="text/css" href="~/Content/upc/common/shared.css?v=3200.0.5-rel.6+3dd6b56" id="css_0">
    <link rel="stylesheet" type="text/css" href="~/Content/upc/branding/themes/UPC2018/theme.css?v=3200.0.5-rel.6+3dd6b56" id="css_1">
    <link rel="stylesheet" type="text/css" href="~/Content/upc/branding/_1_1/brand.css?ts=1514300964000&v=3200.0.5-rel.6+3dd6b56" id="css_2">
    <style type="text/css">
        /* Custom styles go here */
        #loginBox li label,
        .portlet #loginBoxFull li label {
            color: #333;
        }

        #loginBox li input {
            box-shadow: none;
            -moz-box-shadow: none;
            -webkit-box-shadow: none;
        }

        #loginBox input[type="text"],
        #loginBox input[type="password"] {
            -moz-border-radius: 0;
            -webkit-border-radius: 0;
            border-radius: 0;
            background: #efefef;
            padding: 6px;
            border: 2px solid #ddd;
            color: #666;
            font-weight: bold;
            margin: 0 0 0 -10px;
        }

            #loginBox input[type="text"]:focus,
            #loginBox input[type="password"]:focus,
            #loginBox input[type="text"]:hover,
            #loginBox input[type="password"]:hover {
                background: #fff;
                border-color: #E3000F;
                color: #333;
                outline: none;
            }

        #loginOptions {
            margin: 38px auto 26px;
            width: 431px;
            text-align: center;
        }

            #loginOptions ul li a,
            #loginOptions ul li.subOption a {
                font-size: 130%;
                border-bottom: 1px dotted #E3000F;
            }

        #loginFormList .forgot {
            display: none;
        }

        #loginFormList li:first-child + li + li {
            float: right;
        }

        .custom_forgot a {
            color: #000;
            border-bottom: 1px dotted #000
        }

            .custom_forgot a:hover {
                color: #E3000F;
                border-bottom: 1px dotted #E3000F
            }

        .custom_forgot {
            float: left;
            position: relative;
            text-align: left;
            top: -15px;
            left: 80px;
        }

        #custom_forgot_msg {
            border: 2px solid #ccc;
            background: #ffa;
            width: 200px;
            float: left;
            text-align: left;
            margin: 10px 0 0 -45px;
            padding: 10px;
            display: none;
        }
    </style>

    <script type="text/javascript" src="~/Content/upc/groupjs/EAB0C82B128EE3ABB7F3ADB960052484.js?v=3200.0.5-rel.6+3dd6b56"></script>
</head>
<body class='login-page'>

    <div id="loginPageContainer1">
        <div id="loginPane">
            <div id="loginContainer">

                <div class="clearfix loginBody">
                    @Html.ValidationSummary(true, "", new { @class = "receipt bad editmode alert-box alert" })
                    <div id="loginBox">
                        @using (Html.BeginForm("Login", "Account", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { onsubmit = "return validate_form( this, false, true );" }))
                        {

                            @Html.AntiForgeryToken()
                            <div id="loginFormFields" class="clearfix">
                                <ul id="loginFormList" class="clearfix">
                                    <li class="clearfix">
                                        @Html.LabelFor(m => m.user_id)
                                        @Html.TextBoxFor(m => m.user_id, new { size = "25", maxlength = "50" })
                                        @Html.ValidationMessageFor(m => m.user_id, "", new { @class = "text-danger" })

                                    </li>
                                    <li class="clearfix">
                                        @Html.LabelFor(m => m.password)
                                        @Html.PasswordFor(m => m.password, new { autocomplete = "off", size = "25" })
                                        @Html.ValidationMessageFor(m => m.password, "", new { @class = "text-danger" })
                                    </li>
                                    <li class="clearfix">
                                        <input id="entry-login" type="submit"
                                               value="Iniciar sesión"
                                               name="login"
                                               class="button expand"
                                               style="height: 40px;" />
                                    </li>
                                </ul>
                            </div>

                            <input type="hidden" name="action" value="login" />
                            <input type="hidden" name="new_loc" value="" />
                        }
                    </div>
                    <div>
                        <br />
                        <br />
                        <span class="custom_forgot">

                            <a href="https://intranet.upc.edu.pe/LoginIntranet/ResetPassword.aspx" alt="" title="">
                                ¿Olvidaste tu contraseña para alumnos de Pregrado y Epe?
                            </a>
                            <br>
                            <br>

                            <a href="https://intranet-epg.upc.edu.pe/LoginIntranet/ResetPassword.aspx" alt="" title="">
                                ¿Olvidaste tu contraseña para alumnos de Postgrado?
                            </a>

                            <br>
                            <br>
                            <img src="https://aulavirtual.upc.edu.pe/branding/themes/NUEVOUPC/images/banner-aula-virtual.jpg" alt="" class="productLogo">
                        </span>
                    </div>

                    <div id="copyright" class="clearfix">
                        <script type="text/javascript">
                            function showCopyrightDetail() {
                                if (document.getElementById('copyrightDetail').style.display == "none" || document.getElementById('copyrightDetail').style.display == "") { document.getElementById('copyrightDetail').style.display = "block"; }
                                else { document.getElementById('copyrightDetail').style.display = "none"; }
                            }
                        </script>
                        <div class="newCopyright">
                            <div class="logo">
                                <img src="https://aulavirtual.upc.edu.pe/images/ci/logos/bb_signature/Bb_Signature_StdMed.png" alt="Blackboard Learn">
                            </div>
                            <div class="legal">
                                © 1997-2018 Blackboard Inc. Todos los derechos reservados. Número de patente 7.493.396 y 7.558.853 de Estados Unidos. Resto de patentes en trámite.
                            </div>
                            <div class="additionalInfo">
                                <p><a href="http://access.blackboard.com" id="accessibilityLink">Información sobre accesibilidad</a></p>
                                <p><a href="Javascript:showCopyrightDetail()">Detalles de instalación</a></p>
                            </div>
                        </div>

                        <div class="copyrightDetailContents clearfix" id="copyrightDetail">
                            <a class="closeCopyright" href="#" onclick="showCopyrightDetail()"></a>
                            <img src="https://aulavirtual.upc.edu.pe/images/ci/logos/bb_signature/Bb_Signature_StdMed.png" class="productImg" alt="Blackboard Learn">
                            <ul>
                                <li><i>Blackboard Learn ™</i> </li>
                                <li>Course Delivery (3200.0.5-rel.6+3dd6b56)</li>
                                <li> Community Engagement (3200.0.5-rel.6+3dd6b56)</li>
                                <li> Content Management (3200.0.5-rel.6+3dd6b56)</li>
                                <li><span>Tema: UPC2018</span></li>
                            </ul>
                        </div>
                    </div>

                </div>
            </div>
        </div>

    </div>
    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }

    <script type="text/javascript">
        function loadLoginPage() {
            if (top != self) {
                top.location.replace(self.location.href);
            }
            if (document.forms.login.user_id != undefined) {
                document.forms.login.user_id.focus();
            }
            setTimeout("triggerScreenreaderAlert()", 500);
        }

        function triggerScreenreaderAlert() {
            if (document.getElementById('loginErrorMessage')) {
                $('loginErrorMessage').update($('loginErrorMessage').innerHTML);
            }
        }
    </script>
</body>
</html>




