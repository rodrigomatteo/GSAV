    <script type="text/javascript" src="~/Content/upc/groupjs/0B1FC17DEDC3A018704EC34651A58AB9.js?v=3900.19.0-rel.35+4040100"></script>
    <script type="text/javascript" src="~/Content/upc/ui-ultra/js/hide-focus-outline.js?v=3900.19.0-rel.35+4040100"></script>
    <script type="text/javascript" src="~/Content/upc/ui-ultra/js/login-page.js?v=3900.19.0-rel.35+4040100"></script>
    <script type="text/javascript" src="~/Content/upc/groupjs/AC879C98C04B4D84F7C9F97BBF61F4D8.js?v=3900.19.0-rel.35+4040100"></script>
</head>
<body id="learn-oe-body" class="bb-login hide-focus-outline">
    <!-- If you wanted to call your own css file and keep the css outside of the login.jsp file you could call it from here -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Raleway:300,400&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Oswald:wght@200;500&display=swap" rel="stylesheet">
    <!-- INSERT YOUR CUSTOM CSS IN THIS SECTION IF PROVIDING INLINE CSS. The bbng:cssBlock tag does not come in the default template file. You need to add it separately -->
    <!-- END OF CUSTOM CSS -->
    <div class="container" id="mainblocks">
        <div class="row justify-content-center">
            <div class="col-lg-8 col-md-8 col-sm-12" id="loginblockmain">
                <div id="login-block"
                     class="loginblocks small-11 small-centered medium-12 medium-centered text-center columns">
                    <!-- This is the default Blackboard logo which appears in/above the login block -->
                    <h1 class="login-logo new-logo customized-logo">
                        <span>Blackboard Learn ™</span>
                        <img src="~/Content/upc/branding/_1_1/loginLogo/CustomLoginLogo.png?m=kox2ycsx" alt="" />
                    </h1>
                    <!-- the "login-form" contains the username and password fields in the login block -->
                    <div id="login-form" class="login-form">
                        
                            @Html.ValidationSummary(true, "", new { @class = "receipt bad editmode alert-box alert" })
                        

                        @using (Html.BeginForm("Login", "Account", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { onsubmit = "return validate_form( this, false, true );" }))
                        {

                            @Html.AntiForgeryToken()
                                <script type="text/javascript">
                                    function login_openForgotPassword(url) {
                                        var passwordWin = window.open(url, 'forgotPasswordWindow', 'menubar=1,resizable=1,scrollbars=1,status=1,width=850,height=480');
                                        passwordWin.focus();
                                    }
                                </script>
                                <div id="loginFormFields" class="clearfix">
                                    <ul id="loginFormList" class="clearfix">
                                        <li class="clearfix">
                                            @Html.LabelFor(m => m.user_id)
                                            @Html.TextBoxFor(m => m.user_id, new { size = "25", maxlength = "50" })
                                            @Html.ValidationMessageFor(m => m.user_id, "", new { @class = "text-danger" })
                                        </li>
                                        <li class="clearfix">
                                            @Html.LabelFor(m => m.password)
                                            @Html.PasswordFor(m => m.password, new { autocomplete = "off", size = "25" })
                                            @Html.ValidationMessageFor(m => m.password, "", new { @class = "text-danger" })
                                        </li>
                                        <li class="clearfix">
                                            <input id="entry-login" type="submit" value="Iniciar sesión" name="login"
                                                   class="button expand inverse outline">
                                        </li>
                                    </ul>

                                    <div class="login-form-footer row collapse">
                                        <div class="stay-signed-in small-6 columns text-left">
                                        </div>
                                        <div class="small-6 columns text-right">
                                            <a href="https://aulavirtual.upc.edu.pe/webapps/blackboard/password">
                                                ¿Ha
                                                olvidado su contraseña?
                                            </a>
                                        </div>
                                    </div>

                                </div>
                                <input type="hidden" name="action" value="login">
                                <input type="hidden" name="new_loc" value="">
                                <input type="hidden" name="blackboard.platform.security.NonceUtil.nonce"
                                       value="ac20ce67-0646-4a63-a4d4-c3e83e971cb4">
                            
                        }
                        </div>
                    <a style="color:#737373; text-decoration:none;" href="https://micuenta.upc.edu.pe/Home/Index?_f=1"
                       target="_blank">
                        <p style="font-family: 'Oswald', sans-serif; text-transform: uppercase; text-align: center;">
                            ¿OLVIDASTE TU contraseÑa?<br>
                        </p>
                    </a>
                </div>
                <!-- login-page-announcements contains system announcements displayed on the login page. This appears below the "login-block" by default. -->

                <div class="login-page-announcements">
                    <div id="loginAnnouncements">
                        <h3>Anuncios de sistema</h3>
                        <ul>
                            <li>
                                <strong>COVID-19: Si te cuidas, nos cuidamos todos</strong>
                                <em class="announcementDate">
                                    (miércoles 18 de agosto de 2021)
                                </em>
                                <br>
                                <div class="vtbegenerated">
                                    <p>Recuerda que si tú o algún familiar directo presentan síntomas, o se encuentran enfermos(as) con COVID-19, por favor escribe un correo informando a: <a href="mailto:covid_alumnosUPC@upc.pe">covid_alumnosUPC@upc.pe</a>, y también coméntaselo a tu director de carrera.</p>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="bbResources">
                    <div style="text-align: center;" class="blockBTNlogin">
                        <p style="font-family: 'Oswald', sans-serif;">Disponible en iOS o Android:</p>
                        <p>
                            <a class="appimg" href="https://itunes.apple.com/app/blackboard-app/id950424861?mt=8"
                               target="_blank">
                                <img src="~/Content/upc/bbcswebdav/institution/Branding/New_Login/appstore.png" alt="iTunes" width="70"
                                     height="25" style="margin: 5px;">
                            </a>
                            <a class="appimg"
                               href="https://play.google.com/store/apps/details?id=com.blackboard.android.bbstudent&amp;hl=en"
                               target="_blank">
                                <img src="~/Content/upc/bbcswebdav/institution/Branding/New_Login/google-play.png" alt="Google Play"
                                     width="70" height="25" style="margin: 5px;">
                            </a>
                        </p>
                    </div>
                </div>
            </div>
            <!-- this class contains the course catalog styling if course catalog is enabled. -->
            <div class="container" id="footernew">
                <div class="small-12 small-centered text-center columns">

                    <!-- this calls the copyright notice -->
                    <div id="copyright" class="clearfix">
                        <script type="text/javascript">
                            function showCopyrightDetail() {
                                if (document.getElementById('copyrightDetail').style.display == "none" || document.getElementById('copyrightDetail').style.display == "") { document.getElementById('copyrightDetail').style.display = "block"; }
                                else { document.getElementById('copyrightDetail').style.display = "none"; }
                            }
                        </script>
                        <div class="newCopyright">
                            <div class="legal">
                                © 1997-2021 Blackboard Inc. Todos los derechos reservados.
                            </div>
                        </div>
                    </div>
                    <div id="loginOptions">
                        <div id="gatewayButtons">
                            <ul class="inline-list">
                                <li id="help-link">
                                    <a href="https://help.blackboard.com/es-es" target="_blank"
                                       title="Ayuda de inicio de sesión de Bb">Ayuda</a>
                                </li>

                                <li id="privacy-link">
                                    <a href="https://aulavirtual.upc.edu.pe/#"
                                       onclick="showPrivacyPolicy();"
                                       title="Privacidad y condiciones de uso">Privacidad y condiciones de uso</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }

    <script type="text/javascript">
        function loadLoginPage() {
            if (top != self) {
                top.location.replace(self.location.href);
            }
            if (document.forms.login.user_id != undefined) {
                document.forms.login.user_id.focus();
            }
            setTimeout("triggerScreenreaderAlert()", 500);
        }

        function triggerScreenreaderAlert() {
            if (document.getElementById('loginErrorMessage')) {
                $('loginErrorMessage').update($('loginErrorMessage').innerHTML);
            }
        }
    </script>
</body>
</html>




