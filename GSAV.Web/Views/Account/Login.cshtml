    <style type="text/css">
        @@charset "utf-8";

        /*------------main quick changes-----------*/
        /*login button*/
        input#entry-login {
            background-color: #ff0000;
        }

        /*background image*/
    body#learn-oe-body { background: url('../../Content/upc/bbcswebdav/institution/Branding/New_Login/fondo-bb.jpg') no-repeat center center fixed; }

        /*background color left block*/
        div#loginblockmain {
            background-color: white;
        }

        /*Background color right block*/
        div#contentblock {
            background-color: #0000007a;
            color: #fff;
            border-radius: 0px 10px 10px 0px;
        }

        /*Create a new account*/
        .trial-registration a {
            color: #ff0000;
        }

        /*Hide or show Forgot Password*/
        .collapse:not(.show) {
            display: none;
        }

        .login-form-footer .text-right a {
            color: #ff0000;

        }

        /*Course Catalogue Button*/
        div#ultraCourseCatalogLink a {
            color: #fff !important;
            background-color: #ff0000;
        }

        /*-----------main body blocks-----------*/
        div#mainblocks {
            max-width: 800px;
            margin: 5% auto;
        }

        div#mainblocks .row {
            margin: 0;
        }

        div#loginblockmain,
        div#contentblock {
            padding: 2em;
        }

        body#learn-oe-body {
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
        }

        /*-------------Login block-------------*/
        .bb-login #login-block {
            margin: 0 auto;
            min-width: 15rem;
            max-width: 20rem;
        }

        .bb-login .login-form ul input[type='text'],
        .bb-login .login-form ul input[type='password'] {
            border: 1px solid black;
            border-radius: 2em;
        }

        .bb-login .login-form ul input[type='text']:focus,
        .bb-login .login-form ul input[type='password']:focus {
            border-bottom-color: black;
        }

        .bb-login .login-form ul label.float-above {
            margin-top: -6px;
            color: #272726;
        }

        .bb-login .login-form ul label {
            margin-left: 10px;
            width: 100%;
            margin-top: 6px;
            left: 0;
        }

        input#entry-login {
            margin-bottom: 2em;
            border-radius: 2em;
            margin-top: 1em;
            width: 100%;
            max-width: 70%;
            color: #fff;
            border: 0px;
            box-shadow: 0 0 0 0px #fff inset;
            font-family: 'Oswald', sans-serif;
        }

    div#loginblockmain { background: url('../../Content/upc/bbcswebdav/institution/Branding/New_Login/bblearn_2011_bg.png') repeat 0 0; border-radius: 10px 10px 10px 10px; }

        .bb-login .login-form ul label {
            color: black;
        }

        input[type="text"],
        input[type="password"],
        input[type="date"],
        input[type="datetime"],
        input[type="datetime-local"],
        input[type="month"],
        input[type="week"],
        input[type="email"],
        input[type="number"],
        input[type="search"],
        input[type="tel"],
        input[type="time"],
        input[type="url"],
        input[type="color"],
        textarea {
            padding: 1rem;
        }

        .bb-login .login-form ul input[type='text'],
        .bb-login .login-form ul input[type='password'] {
            color: black;
        }

        .trial-registration {
            margin-top: 0;
        }

        .login-form-footer .small-6 {
            width: 100% !important;
        }

        .login-form-footer .text-right a {
            text-align: center;
            display: block;
            font-size: 16px;
            width: 100%;
            margin-bottom: 10px;
        }

        div#ultraCourseCatalogLink a {
            text-decoration: none !important;
            padding: 1em 2em;
            margin-bottom: 10px;
            border-radius: 2em;
            font-size: 14px !important;
        }

        div#ultraCourseCatalogLink {
            margin-bottom: 2em;
        }

        /*------------logo-----------------*/
        h1.login-logo.new-logo.customized-logo {
            margin: 5% 0;
        }

        .bb-login .new-logo.customized-logo img {
            width: 100%;
            height: auto;
            border-radius: 0;
            max-width: 70%;
        }

        /*-------------Announcements------------*/
        .bb-login .login-page-announcements {
            margin-top: 0;
            margin-bottom: 2em;
        }

        .bb-login .login-page-announcements #loginAnnouncements>ul>li {
            width: auto;
            border-radius: 2px;
            background-color: #2125291a !important;
            color: #212529;
            border: 1px solid white;
            border-radius: 2em;
            padding: 20px !important;
            max-width: auto;
            display: block;
            padding-top: 24px;
        }

        .bb-login .login-page-announcements #loginAnnouncements>ul>li strong {
            padding: 0;
        }

        div#loginAnnouncements {
            max-height: 210px;
        }

        a.appimg img {
            width: 25%;
            margin: 0 0% !important;
        }

        .bbResources h1 {
            color: #fff;
            font-family: 'Oswald', sans-serif;
        }

        .bb-login .login-page-announcements #loginAnnouncements>ul>li .announcementDate {
            display: block;
            flex: 0 0 100%;
            text-align: justify;
            padding-top: 0;
        }

        .vtbegenerated p {
            font-family: inherit;
            font-weight: 400;
            font-size: 12px !important;
            line-height: 1.6;
            margin-bottom: 1.25rem;
            text-rendering: optimizeLegibility;
            text-align: justify;
        }

        .bb-login .login-page-announcements #loginAnnouncements>ul>li .vtbegenerated {
            max-height: auto;
            padding-right: 0px;
            margin-top: 15px;
            overflow-y: initial;
            flex: 0 0 100%;
        }

        /*------------Footer-----------*/
        .bb-login #gatewayButtons {
            margin-bottom: 0;
        }

        div#gatewayButtons ul {
            margin-bottom: 0;
        }

        div#gatewayButtons ul li a {
            color: white !important;
            font-weight: bold;
        }

        .bb-login #copyright {
            margin-top: 15px;
            color: white;
        }

        #footernew {
            bottom: 0;
            text-align: center;
        }

        div#loginOptions,
        div#copyright {
            margin: 0px auto;
        }

        /*---------Browser Edits-------*/
        .row:before,
        .row:after {
            display: flex !important;
        }

        /*-----------media queries-----------*/
        @@media (max-width: 576px) {
            div#loginblockmain {
                margin: 0;
            }
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        #btn-mas {
            display: none;
        }

        .sticky {
            position: fixed;
            right: 0px;
            top: 230px;

        }

        .redes a,
        .icon-mas2 {
            display: block;
            text-decoration: none;
            background: #ffffff;
            color: #fff;
            width: 150px;
            height: 55px;
            line-height: 55px;
            text-align: center;
            box-shadow: 0px 1px 10px rgba(0, 0, 0, 0.4);
            transition: all 500ms ease;
            border-radius: 10px 0px 0px 10px;
            font-family: 'Oswald', sans-serif;
        }

        .redes a:hover {
            background: rgb(47, 47, 47);
            color: #ffffff;
        }

        .redes a {
            margin-bottom: -15px;
            opacity: 0;
            visibility: hidden;
        }

        #btn-mas:checked~.redes a {
            margin-bottom: 10px;
            opacity: 1;
            visibility: visible;
        }

        .icon-mas2 {
            cursor: pointer;
            background: #ff0000;
            font-size: 12px;
            width: 150px;
        }

        #btn-mas:checked~.btn-mas .icon-mas2 {
            font-size: 12px;
        }

        a.pregrado {
            color: #ff0000;
            height: 50px;
            font-size: 15px;
        }

        a.posgrado {
            color: black;
            height: 50px;
            font-size: 15px;
        }

        .correo {
            height: 50px;
            color: #fff !important;
            text-align: center;
            text-decoration: none !important;
        }

        a.appimg img {
            width: 30%;
            margin: 0 0% !important;
        }
    </style>

